    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs-es": "^5.0.0-beta.12",
    "temp": "^0.8.3",
    "ts-node": "^1.7.2",
    "tslint": "^4.1.1",
    "typescript": "2.0.10"
  }
