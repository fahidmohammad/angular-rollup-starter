{
  "name": "angular2-rollup-starter",
  "version": "0.0.59",
  "description": "Angular2 Starter based on Rollup",
  "scripts": {
    "serve": "node ./scripts/cmd.js serve",
    "start": "npm run serve",
    "roll": "node ./scripts/cmd.js dist",
    "build": "npm run roll",
    "roll:prerender": "node ./scripts/cmd.js dist prerender",
    "clean": "node ./scripts/cmd.js clean",
    "test": "karma start config/karma.conf.js",
    "test:build": "node ./tests/run.js",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor ./config/protractor.conf.js",
    "pree2e": "npm run webdriver:update -- --standalone",
    "protractor": "protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "ci": "npm run lint && npm test && npm run e2e",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "tslint": "tslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkuri/angular2-rollup-starter.git"
  },
  "keywords": [
    "Angular2",
    "ng2",
    "rollup",
    "rollupjs",
    "browser-sync"
  ],
  "author": "Jan Kuri <jkuri88@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkuri/angular2-rollup-starter/issues"
  },
  "homepage": "https://github.com/jkuri/angular2-rollup-starter#readme",
  "dependencies": {
    "@angular/common": "4.0.0-beta.1",
    "@angular/compiler": "4.0.0-beta.1",
    "@angular/compiler-cli": "4.0.0-beta.1",
    "@angular/core": "4.0.0-beta.1",
    "@angular/forms": "4.0.0-beta.1",
    "@angular/http": "4.0.0-beta.1",
    "@angular/platform-browser": "4.0.0-beta.1",
    "@angular/platform-browser-dynamic": "4.0.0-beta.1",
    "@angular/platform-server": "4.0.0-beta.1",
    "@angular/router": "3.4.1",
    "angular2-universal": "2.1.0-rc.1",
    "core-js": "2.4.1",
    "ng2-platform-node": "0.0.5",
    "rxjs": "5.0.1",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/browser-sync": "0.0.34",
    "@types/chalk": "^0.4.31",
    "@types/fs-extra": "0.0.35",
    "@types/glob": "^5.0.30",
    "@types/jasmine": "^2.5.38",
    "@types/lodash": "^4.14.44",
    "@types/node": "^6.0.52",
    "@types/node-sass": "3.10.32",
    "@types/selenium-webdriver": "^2.53.37",
    "@types/temp": "^0.8.29",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "cli-progress-spinner": "0.0.2",
    "codelyzer": "2.0.0-beta.4",
    "fs-extra": "^1.0.0",
    "glob": "^7.1.1",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-plugin": "0.2.4",
    "karma-spec-reporter": "0.0.26",
    "livereload": "^0.6.0",
    "lodash": "^4.17.2",
